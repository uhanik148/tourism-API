{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "paths": {
      "@/": ["./src/"]
      
    }
  },
  "include": ["next-env.d.ts", "/.ts", "/.tsx"],,
  "exclude": ["node_modules"]
}
